generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model DistrictData {
  id                                       Int      @id @default(autoincrement())
  fin_year                                 String
  month                                    String
  state_code                               String?
  state_name                               String?
  district_code                            String
  district_name                            String

  approved_labour_budget                   Float?
  average_wage_rate_per_day_per_person     Float?
  average_days_of_employment_per_household Float?
  differently_abled_persons_worked         Int?
  material_and_skilled_wages               Float?
  number_of_completed_works                Int?
  number_of_gps_with_nil_exp               Int?
  number_of_ongoing_works                  Int?
  persondays_of_central_liability_so_far   Float?
  sc_persondays                            Float?
  sc_workers_against_active_workers        Float?
  st_persondays                            Float?
  st_workers_against_active_workers        Float?
  total_adm_expenditure                    Float?
  total_exp                                Float?
  total_households_worked                  Int?
  total_individuals_worked                 Int?
  total_no_of_active_job_cards             Int?
  total_no_of_active_workers               Int?
  total_no_of_hhs_completed_100_days       Int?
  total_no_of_jobcards_issued              Int?
  total_no_of_workers                      Int?
  total_no_of_works_takenup                Int?
  wages                                    Float?
  women_persondays                         Float?
  percent_of_category_b_works              Float?
  percent_of_expenditure_on_agri_works     Float?
  percent_of_nrm_expenditure               Float?
  percentage_payments_generated_within_15_days Float?
  remarks                                  String?

  @@unique([fin_year, month, district_code])
}
